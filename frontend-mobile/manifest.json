  "versionName": "1.0.0",
  "versionCode": "100",
  "transformPx": false,

  "mp-weixin": {
    "appid": "wxXXXXXXXXXXXXXXXX",
    "setting": {
      "urlCheck": false,
      "es6": true,
      "enhance": true,
      "postcss": true,
      "minified": true,
      "newFeature": true,
      "bigPackageSizeSupport": true
    },
