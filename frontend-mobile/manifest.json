  "h5": {
    "title": "西藏电信仓库管理",
    "template": "index.html",
    "router": {
      "mode": "hash",
      "base": "/"
    },
    "optimization": {
      "treeShaking": {
        "enable": true
      }
    }
  },

  "vueVersion": "3"
